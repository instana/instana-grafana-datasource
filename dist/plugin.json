{
  "name": "Instana",
  "id": "instana-datasource",
  "type": "datasource",

  "partials": {
    "config": "public/app/plugins/datasource/instana/partials/config.html"
  },

  "metrics": true,
  "annotations": true,

  "info": {
    "description": "instana datasource",
    "author": {
      "name": "Instana inc",
      "url": "https://instana.com"
    },
    "logos": {
      "small": "https://www.instana.com/media/instana_icon_square.png",
      "large": "https://www.instana.com/media/instana_icon_square.png"
    },
    "links": [
      {"name": "GitHub", "url": "https://instana.com" },
      {"name": "MIT License", "url": "https://instana.com" }
    ],
    "version": "1.2.0",
    "updated": "2018-09-19"
  },

  "routes": [
    {
      "path": "instana",
      "method": "GET",
      "url": "{{.JsonData.url}}",
      "headers": [
          {"name": "Authorization", "content": "apiToken {{.JsonData.apiToken}}"}
      ]
    }
  ],

  "dependencies": {
    "grafanaVersion": "3.x.x",
    "plugins": [ ]
  }
}
