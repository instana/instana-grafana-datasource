{
  "type": "datasource",
  "name": "Instana",
  "id": "instana-datasource",
  "metrics": true,
  "annotations": false,
  "alerting": false,
  "logs": false,
  "info": {
    "description": "Grafana datasource plugin for Instana: Automatic Infrastructure and Application Monitoring",
    "author": {
      "name": "Instana, Inc.",
      "url": "https://www.instana.com"
    },
    "logos": {
      "small": "https://www.instana.com/media/instana_icon_square.png",
      "large": "https://www.instana.com/media/instana_icon_square.png"
    },
    "links": [
      {
        "name": "Docs",
        "url": "https://docs.instana.io/ecosystem/grafana/"
      },
      {
        "name": "Homepage",
        "url": "https://www.instana.com"
      },
      {
        "name": "GitHub",
        "url": "https://github.com/instana/instana-grafana-datasource"
      },
      {
        "name": "Apache License 2.0",
        "url": "https://github.com/instana/instana-grafana-datasource/blob/master/LICENSE"
      }
    ],
    "screenshots": [
      {
        "name": "Infrastructure Built-In Metrics",
        "path": "https://raw.githubusercontent.com/instana/instana-grafana-datasource/master/screenshots/built-in-metrics.gif"
      }
    ],
    "version": "3.0.0",
    "updated": "2020-08-12"
  },
  "routes": [
    {
      "path": "instana",
      "method": "*",
      "url": "{{.JsonData.url}}",
      "headers": [
        {
          "name": "Authorization",
          "content": "apiToken {{.JsonData.apiToken}}"
        }
      ]
    }
  ],
  "dependencies": {
    "grafanaVersion": "7.0.x",
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  }
}
